extends base.pug
// TODO replace MODEL with the model name

- load i18n

block breadcrumbs
  li
    a(href="{% url 'MODEL_list' %}")
      | {% trans 'MODEL list'|capfirst %}

block content
  .text-right
    .btn-group
      a.btn.btn-primary(href="{% url 'MODEL_update' MODEL.id %}")
        i.fa.fa-edit
      a.btn.btn-danger(href="{% url 'MODEL_delete' MODEL.id %}")
        i.fa.fa-times

  table.table.table-bordered.table-hover
    tbody
      th Id
      td {{MODEL.id}}
